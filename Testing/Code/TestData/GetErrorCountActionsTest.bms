# This script should exit without an error.
Echo('no errors so far')
GetErrorCount(numErr)
If( ${numErr} != 0 )
  AddError( "shouldn't have had any errors yet.")
  Exit()
EndIf( ${numErr} )
AddError('error1')
AddError('error2')
AddError('error3')
AddError('error4')
GetErrorCount(numErr)
Echo('numErr: ' ${numErr})
If( ${numErr} != 4 )
  AddError( "should have had 4 errors.")
  Exit()
EndIf( ${numErr} )
ClearErrors()
GetErrorCount(numErr)
If( ${numErr} != 0 )
  AddError( "shouldn't have had any errors.")
  Exit()
EndIf( ${numErr} )
Exit()
