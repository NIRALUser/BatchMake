ExitOnError( '1' )
set( string 'my car is green, really green.' )
Set( regexp '(g[^n]*n).*(g[^n]*n)' )
echo( ${regexp} )
RegEx( result ${string} ${regexp} MATCH )
echo( '"'${string}'"' ' matches "'${regexp}'". "'${result}'" is found' )
RegEx( result ${string} ${regexp} REPLACE 'blue\0red\1' )
echo( $result )
RegEx( result 'foo' 'o' REPLACE 'a' )