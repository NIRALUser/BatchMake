ExitOnError( '1' )
# basic inc
Set( calc '15.3' )
Set( res ${calc} )
Inc( ${res} 0.9 )
Echo( ${calc} ' + 0.9 = ' ${res} )
# using a variable to inc
Set( calc '18' )
Set( increment '2' )
Set( res ${calc} )
Inc( ${res} ${increment} )
Echo( ${calc} ' + ' ${increment} ' = ' ${res} )
# using sequences
Sequence( seq 0 10 2 )
Echo('Sequence: ' ${seq})
GetParamCount( size ${seq} )
Echo( 'Sequence size: ' ${size} )
ForEach(arg ${seq})
 Inc( ${arg} 1 )
 set( newseq ${newseq} ${arg} )
EndForEach(arg)
Echo('New sequence: ' ${newseq})